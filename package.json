{
  "name": "tays",
  "version": "0.0.1",
  "description": "Does Stuff",
  "main": "lib/index.js",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/benstepp/tays.git"
  },
  "scripts": {
    "build": "node_modules/.bin/babel lib --out-dir dist/ --source-maps inline",
    "test": "node_modules/.bin/mocha 'test/**/*_test.js' --harmony-proxies --reporter dot --require test/test_helper.js",
    "test-watch": "node_modules/.bin/mocha 'test/**/*_test.js' --harmony-proxies --reporter dot --require test/test_helper.js --watch",
    "coverage": "node_modules/.bin/istanbul cover _mocha -- 'test/**/*_test.js' --harmony-proxies --reporter dot --require test/test_helper.js; open coverage/lcov-report/index.html",
    "prepublish": "npm run test && npm run build"
  },
  "keywords": ["orm", "database", "active record"],
  "author": "Benjamin Stepp <me@benstepp.com> (http://fightingdragonswithtaylorswift.com/)",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-plugin-transform-async-to-generator": "^6.4.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.4.0",
    "babel-plugin-transform-es2015-parameters": "^6.4.2",
    "babel-plugin-transform-export-extensions": "^6.4.0",
    "babel-register": "^6.3.13",
    "chai": "^3.4.1",
    "harmony-reflect": "^1.4.2",
    "istanbul": "1.0.0-alpha.2",
    "mocha": "^2.3.4",
    "sinon": "^1.17.3"
  },
  "dependencies": {
    "lodash": "^4.6.0"
  }
}
